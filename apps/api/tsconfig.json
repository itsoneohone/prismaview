{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2022",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "resolveJsonModule": true,
    "paths": {
      "@/*": ["./src/*"],
      "@access-key/*": ["src/access-key/*"],
      "@app/*": ["src/app/*"],
      "@auth/*": ["src/auth/*"],
      "@commands/*": ["src/commands/*"],
      "@events/*": ["src/events/*"],
      "@lib/*": ["src/lib/*"],
      "@order/*": ["src/order/*"],
      "@playground/*": ["src/playground/*"],
      "@price/*": ["src/price/*"],
      "@prismaModule/*": ["src/prisma/*"],
      "@scheduler/*": ["src/scheduler/*"],
      "@shared/*": ["src/shared/*"],
      "@user/*": ["src/user/*"]
    }
  }
}
