{
  "name": "api",
  "version": "0.0.1",
  "description": "PrismaView API",
  "author": "",
  "license": "GNU GPL V3",
  "scripts": {
    "app:dev:start": "nest start",
    "app:dev:watch": "nest start --watch",
    "app:dev:debug": "nest start --debug --watch",
    "app:prod:start": "node dist/main",
    "app:build": "nest build",
    "cli:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register src/cli.ts",
    "cli:start": "pnpm exec ts-node -r tsconfig-paths/register src/cli.ts",
    "db:dev:migrate": "prisma migrate dev",
    "db:dev:rm": "docker-compose rm dev-db -s -f -v",
    "db:dev:up": "docker-compose up dev-db -d",
    "db:dev:recreate": "pnpm run db:dev:rm && pnpm run db:dev:up && sleep 1 && pnpm run db:dev:migrate",
    "db:test:migrate": "dotenv -o -e .env.test -- prisma migrate dev",
    "db:test:rm": "docker-compose rm test-db -s -f -v",
    "db:test:up": "docker-compose up test-db -d",
    "db:test:recreate": "pnpm run db:test:rm && pnpm run db:test:up && sleep 1 && pnpm run db:test:migrate",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:write": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint:check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:run": "rm -rf dist && jest",
    "test:watch": "jest --watchAll",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "dotenv -e .env.test -- jest --config ./test/jest-e2e.json --watch"
  },
  "dependencies": {
    "@keyv/redis": "^4.4.0",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.1.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.1",
    "@nestjs/platform-socket.io": "^11.1.1",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/websockets": "^11.1.1",
    "@prisma/client": "^6.8.2",
    "@types/hbs": "^4.0.4",
    "@types/lodash": "^4.17.17",
    "argon2": "^0.43.0",
    "axios": "^1.9.0",
    "cache-manager": "^6.4.3",
    "cacheable": "^1.9.0",
    "ccxt": "^4.4.85",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "connect-redis": "^8.1.0",
    "express-session": "^1.18.1",
    "hbs": "^4.2.0",
    "keyv": "^5.3.3",
    "lodash": "^4.17.21",
    "nest-commander": "^3.17.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "prisma": "^6.8.2",
    "redis": "^5.1.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "socket.io": "^4.8.1",
    "source-map-support": "^0.5.21",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.27.0",
    "@faker-js/faker": "^9.8.0",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.1",
    "@types/express": "^5.0.2",
    "@types/express-session": "^1.18.1",
    "@types/hbs": "^4.0.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.21",
    "@types/passport-jwt": "^4.0.1",
    "@types/source-map-support": "^0.5.10",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "globals": "^16.1.0",
    "jest": "^29.7.0",
    "jest-mock-extended": "^4.0.0-beta1",
    "pactum": "^3.7.6",
    "prettier": "^3.5.3",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "modulePaths": [
      "<rootDir>"
    ],
    "moduleNameMapper": {
      "@/(.*)": "<rootDir>/src/$1",
      "@access-key/(.*)": "<rootDir>/src/access-key/$1",
      "@auth/(.*)": "<rootDir>/src/auth/$1",
      "@lib/(.*)": "<rootDir>/src/lib/$1",
      "@order/(.*)": "<rootDir>/src/order/$1",
      "@prismaModule/(.*)": "<rootDir>/src/prisma/$1",
      "@scheduler/(.*)": "<rootDir>/src/scheduler/$1",
      "@shared/(.*)": "<rootDir>/src/shared/$1",
      "@user/(.*)": "<rootDir>/src/user/$1"
    },
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "globalSetup": "<rootDir>/test/setup.ts",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ]
  }
}