{
  "name": "prismaview",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "GNU GPL V3",
  "scripts": {
    "prisma:dev:deploy": "prisma migrate deploy",
    "db:dev:rm": "docker-compose rm dev-db -s -f -v",
    "db:dev:up": "docker-compose up dev-db -d",
    "db:dev:restart": "npm run db:dev:rm && npm run db:dev:up && sleep 1 && npm run prisma:dev:deploy",
    "prisma:test:deploy": "dotenv -o -e .env.test -- prisma migrate deploy",
    "db:test:rm": "docker-compose rm test-db -s -f -v",
    "db:test:up": "docker-compose up test-db -d",
    "db:test:restart": "npm run db:test:rm && npm run db:test:up && sleep 1 && npm run prisma:test:deploy",
    "build": "nest build",
    "prisma:deploy": "prisma migrate deploy",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "pretest:e2e": "npm run db:test:restart",
    "test:e2e": "dotenv -e .env.test -- jest --config ./test/jest-e2e.json --watch"
  },
  "dependencies": {
    "@keyv/redis": "^4.2.0",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/cache-manager": "^3.0.0",
    "@nestjs/common": "^11.0.7",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.7",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.7",
    "@nestjs/platform-socket.io": "^11.0.7",
    "@nestjs/schedule": "^5.0.1",
    "@nestjs/websockets": "^11.0.7",
    "@prisma/client": "^6.3.0",
    "@types/hbs": "^4.0.4",
    "@types/lodash": "^4.17.15",
    "argon2": "^0.41.1",
    "axios": "^1.7.9",
    "cache-manager": "^6.4.0",
    "cacheable": "^1.8.8",
    "ccxt": "^4.4.53",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "connect-redis": "^8.0.1",
    "express-session": "^1.18.1",
    "hbs": "^4.2.0",
    "lodash": "^4.17.21",
    "nest-commander": "^3.16.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "prisma": "^6.3.0",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "source-map-support": "^0.5.21",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.4.0",
    "@nestjs/cli": "^11.0.2",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.7",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.1",
    "@types/hbs": "^4.0.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.12.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/source-map-support": "^0.5.10",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "jest": "^29.7.0",
    "pactum": "^3.7.6",
    "prettier": "^3.4.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "modulePaths": [
      "<rootDir>"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "globalSetup": "<rootDir>/test/setup.ts"
  },
  "engines": {
    "node": ">=22.x"
  }
}
